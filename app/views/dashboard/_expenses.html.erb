<%= render_card(title: expenses_title, subtitle: expenses_subtitle, class: 'w-full space-y-4 px-6') do %>
  <%= turbo_frame_tag 'expenses_list' do %>
    <% if @expenses.any? %>
      <%= render partial: "expense", collection: @expenses, as: :expense %>
    <% else %>
      <p class="text-center">No hay gatos este mes</p>
    <% end %>
  <% end %>
<% end %>